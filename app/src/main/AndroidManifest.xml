<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.cardview">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity android:name=".shops"></activity>
        <activity android:name=".basal" />
        <activity android:name=".pythium" />
        <activity android:name=".fire" />
        <activity android:name=".tulips" />
        <activity android:name=".sarkari" />
        <activity
            android:name=".powder"
            android:parentActivityName=".rose"
            android:theme="@style/Apptheme" />
        <activity
            android:name=".mosaic"
            android:parentActivityName=".rose"
            android:theme="@style/Apptheme" />
        <activity
            android:name=".roset"
            android:parentActivityName=".rose"
            android:theme="@style/Apptheme" />
        <activity
            android:name=".blackrust"
            android:parentActivityName=".rose"
            android:theme="@style/Apptheme" />
        <activity
            android:name=".rose"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Apptheme" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.soundcloud.android.crop.CropImageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Classify"
            android:theme="@style/Apptheme" />
    </application>

</manifest>